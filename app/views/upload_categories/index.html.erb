<p id="notice"><%= notice %></p>

<h1>Listing Upload Categories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @upload_categories.each do |upload_category| %>
      <tr>
        <td><%= upload_category.name %></td>
        <td><%= link_to 'Show', upload_category %></td>
        <td><%= link_to 'Edit', edit_upload_category_path(upload_category) %></td>
        <td><%= link_to 'Destroy', upload_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Upload category', new_upload_category_path %>
