<p id="notice"><%= notice %></p>

<h1>Listing Callers</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Address</th>
      <th>Job</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @callers.each do |caller| %>
      <tr>
        <td><%= caller.first_name %></td>
        <td><%= caller.last_name %></td>
        <td><%= caller.email %></td>
        <td><%= caller.address_id %></td>
        <td><%= caller.job_id %></td>
        <td><%= link_to 'Show', caller %></td>
        <td><%= link_to 'Edit', edit_caller_path(caller) %></td>
        <td><%= link_to 'Destroy', caller, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Caller', new_caller_path %>
