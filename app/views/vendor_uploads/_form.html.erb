<%= form_for [@vendor, @vendor_upload] do |f| %>
<% if @vendor_upload.errors.any? %>
<div id="error_explanation">
    <h2><%= pluralize(@vendor_upload.errors.count, "error") %>
        prohibited this vendor_upload from being saved:</h2>

    <ul>
        <% @vendor_upload.errors.full_messages.each do |message| %>
        <li><%= message %></li>
        <% end %>
    </ul>
</div>
<% end %>


<div class="col-sm-6 col-sm-offset-3">
    <div class="form-group">
        <%= f.label :vendor_upload_type_id %><br>
        <%= f.collection_select :vendor_upload_type_id, VendorUploadType.all, :id, :name, {:prompt => 'Select an Upload Type'},  { class: 'form-control'} %>
    </div>
</div>
<div class="col-sm-6 col-sm-offset-3">
    <div class="form-group">
        <%= f.label :expiration_date %><br>
        <%= f.date_select :expiration_date %>
    </div>
</div>
<div class="col-sm-6 col-sm-offset-3">
    <div class="form-group">
        <%= f.label :name %><br>
        <%= f.text_field :name, class: "form-control" %>
    </div>
</div>
<div class="col-sm-6 col-sm-offset-3">
    <div class="form-group">
        <%= f.label :notes %><br>
        <%= f.text_area :notes, class: "form-control" %>
    </div>
</div>
<div class="col-sm-6 col-sm-offset-3">
    <div class="form-group">
        <%= f.label :upload %><br>
        <%= f.file_field :upload %>
    </div>
</div>
<div class="col-sm-12 text-center">
    <div class="actions">
        <%= f.submit class: "btn btn-primary"%>
    </div>
</div>
<% end %>
