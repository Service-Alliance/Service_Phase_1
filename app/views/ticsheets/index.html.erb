<p id="notice"><%= notice %></p>

<h1>Listing Ticsheets</h1>

<table>
  <thead>
    <tr>
      <th>Key code</th>
      <th>Description</th>
      <th>Uom</th>
      <th>Percentage</th>
      <th>Quantity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ticsheets.each do |ticsheet| %>
      <tr>
        <td><%= ticsheet.key_code %></td>
        <td><%= ticsheet.description %></td>
        <td><%= ticsheet.uom %></td>
        <td><%= ticsheet.percentage %></td>
        <td><%= ticsheet.quantity %></td>
        <td><%= link_to 'Show', job_ticsheet_path(@job, ticsheet) %></td>
        <td><%= link_to 'Edit', edit_job_ticsheet_path(@job, ticsheet) %></td>
        <td><%= link_to 'Destroy', job_ticsheet_path(@job, ticsheet), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ticsheet', new_job_ticsheet_path %>
