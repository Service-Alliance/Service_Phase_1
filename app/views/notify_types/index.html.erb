<p id="notice"><%= notice %></p>

<h1>Listing Notify Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @notify_types.each do |notify_type| %>
      <tr>
        <td><%= notify_type.name %></td>
        <td><%= link_to 'Show', notify_type %></td>
        <td><%= link_to 'Edit', edit_notify_type_path(notify_type) %></td>
        <td><%= link_to 'Destroy', notify_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Notify type', new_notify_type_path %>
