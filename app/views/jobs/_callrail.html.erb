<div class="panel panel-info">
    <div class="panel-heading">
        <h3 class="panel-title">
          <a data-toggle="collapse" href="#collapseCallrail">CallRail:</a>
        </h3>
    </div>
    <div id="collapseCallrail" class="panel-collapse collapse">
    <div class="panel-body">
        <div class="row">
            <div class="col-xs-12">
                <table class="table">
                    <tbody>
                      <tr>
                          <td>Call Source:</td>
                          <td><%= @callrail.try(:callsource) %></td>
                      </tr>
                      <tr>
                          <td>Tracking Number:</td>
                          <td><%= @callrail.try(:trackingnum) %></td>
                      </tr>
                        <tr>
                            <td>Customer Phone Number:</td>
                            <td><%=@callrail.try(:customer_phone_number) %></td>
                        </tr>
                        <tr>
                            <td>Destination Number:</td>
                            <td><%= @callrail.try(:destinationnum) %></td>
                        </tr>
                        <tr>
                            <td>Customer Name</td>
                            <td><%= @callrail.try(:customer_name) %></td>
                        </tr>
                        <tr>
                            <td>Customer City</td>
                            <td><%= @callrail.try(:customer_city) %></td>
                        </tr>
                        <tr>
                            <td>Customer State</td>
                            <td><%= @callrail.try(:customer_state) %></td>
                        </tr>
                        <tr>
                            <td>Customer Zip</td>
                            <td><%= @callrail.try(:customer_zip) %></td>
                        </tr>
                        <tr>
                            <td>Keywords</td>
                            <td><%= @callrail.try(:keywords) %></td>
                        </tr>
                        <tr>
                            <td>Referrer</td>
                            <td><%= @callrail.try(:referrer) %></td>
                        </tr>
                        <tr>
                            <td>Referrer Medium</td>
                            <td><%= @callrail.try(:referrermedium) %></td>
                        </tr>
                        <tr>
                            <td>Landing Page</td>
                            <td><%= @callrail.try(:landingpage) %></td>
                        </tr>
                        <tr>
                            <td>Last Requested URL</td>
                            <td><%= @callrail.try(:last_requested_url) %></td>
                        </tr>
                        <tr>
                            <td>GCLID</td>
                            <td><%= @callrail.try(:gclid) %></td>
                        </tr>
                        <tr>
                            <td>IP</td>
                            <td><%= @callrail.try(:ip) %></td>
                        </tr>
                        <tr>
                            <td>UTM Source</td>
                            <td><%= @callrail.try(:utm_source) %></td>
                        </tr>
                        <tr>
                            <td>UTM Medium</td>
                            <td><%= @callrail.try(:utm_medium) %></td>
                        </tr>
                        <tr>
                            <td>UTM Term</td>
                            <td><%= @callrail.try(:utm_term) %></td>
                        </tr>
                        <tr>
                            <td>UTM Content</td>
                            <td><%= @callrail.try(:utm_content) %></td>
                        </tr>
                        <tr>
                            <td>UTM Campaign</td>
                            <td><%= @callrail.try(:utm_campaign) %></td>
                        </tr>
                        <tr>
                            <td>UTMA</td>
                            <td><%= @callrail.try(:utma) %></td>
                        </tr>
                        <tr>
                            <td>UTMB</td>
                            <td><%= @callrail.try(:utmb) %></td>
                        </tr>
                        <tr>
                            <td>UTMV</td>
                            <td><%= @callrail.try(:utmv) %></td>
                        </tr>
                        <tr>
                            <td>UTMZ</td>
                            <td><%= @callrail.try(:utmz) %></td>
                        </tr>
                        <tr>
                            <td>UTMX</td>
                            <td><%= @callrail.try(:utmx) %></td>
                        </tr>
                        <tr>
                            <td>GA</td>
                            <td><%= @callrail.try(:ga) %></td>
                        </tr>
                        <tr>
                            <td>First Call?</td>
                            <td><%= @callrail.try(:first_call) %></td>
                        </tr>
                        <tr>
                            <td>Recording</td>
                            <td><%= @callrail.try(:recording) %></td>
                        </tr>
                        <tr>
                            <td>Duration</td>
                            <td><%= @callrail.try(:duration) %></td>
                        </tr>


                    </tbody>
                </table>

            </div>
        </div>
    </div>
    <div class="panel-footer">
        <a href="mailto:<%= @caller.email %>" type="button" class="btn btn-sm btn-primary">
            <i class="glyphicon glyphicon-envelope"></i>
        </a>
        <span class="pull-right">
            <%= link_to edit_job_caller_path(@job, @caller), class: "btn btn-sm btn-warning", type: "button" do %>
            <i class="glyphicon glyphicon-edit"></i>
            <% end %>
        </span>
    </div>
  </div>
</div>
