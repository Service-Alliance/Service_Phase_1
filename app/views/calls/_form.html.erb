<%= form_for(@call) do |f| %>
<% if @call.errors.any? %>
<div id="error_explanation">
    <h2><%= pluralize(@call.errors.count, "error") %>
        prohibited this call from being saved:</h2>

    <ul>
        <% @call.errors.full_messages.each do |message| %>
        <li><%= message %></li>
        <% end %>
    </ul>
</div>
<% end %>
<div class="col-xs-4">
    <div class="form-group">
        <div class="field">
            <%= f.label :callrail_id %><br>
            <%= f.text_field :callrail_id %>
        </div>
    </div>
</div>
<div class="col-xs-4">
    <div class="form-group">
        <div class="field">
            <%= f.label :recording %><br>
            <%= f.text_field :recording %>
        </div>
    </div>
</div>
<div class="col-xs-4">
    <div class="form-group">
        <div class="field">
            <%= f.label :job %><br>
            <%= f.number_field :job_id %>
        </div>
    </div>
</div>
<div class="col-xs-4">
    <div class="form-group">
        <div class="field">
            <%= f.label :start_time %><br>
            <%= f.datetime_select :start_time %>
        </div>
    </div>
</div>
<div class="col-xs-4">
    <div class="form-group">
        <div class="field">
            <%= f.label :customer_phone_number %><br>
            <%= f.text_field :customer_phone_number %>
        </div>
    </div>
</div>
<div class="col-xs-4">
    <div class="form-group">
        <div class="field">
            <%= f.label :duration %><br>
            <%= f.number_field :duration %>
        </div>
    </div>
    <div class="col-xs-12 text-center">

        <div class="actions">
            <%= f.submit %>
        </div>
    </div>
    <% end %>
