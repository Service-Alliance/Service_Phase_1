<div class="container-fluid">
    <% if notice %>
    <div class="alert alert-success" role="alert"><%= notice %></div>
    <% end %>
    <h1 class="text-center">Vendor:</h1>
    <div class="col-xs-8 col-xs-offset-2">
        <div class="panel panel-info">
            <div class="panel-heading">
                <h3 class="panel-title">
                    Vendor
                </h3>
            </div>

            <div class="panel-body">
                <div class="row">
                    <div class="col-xs-12">
                        <table class="table">
                            <tbody>
                                <tr>
                                    <td>ID:</td>
                                    <td><%= @vendor.id %></td>
                                </tr>
                                <tr>
                                    <td>Category:</td>
                                    <td><%= @vendor.category_type %></td>
                                </tr>
                                <tr>
                                    <td>Name:</td>
                                    <td><%= @vendor.name %></td>
                                </tr>
                                <tr>
                                    <td>Contact:</td>
                                    <td><%= @vendor.contact %></td>
                                </tr>
                                <tr>
                                    <td>Phone</td>
                                    <td><%= @vendor.phone %></td>
                                </tr>
                                <tr>
                                    <td>Cellphone</td>
                                    <td><%= @vendor.cellphone %></td>
                                </tr>
                                <tr>
                                    <td>Fax</td>
                                    <td><%= @vendor.fax %></td>
                                </tr>
                                <tr>
                                    <td>Address</td>
                                    <td><%= @vendor.address %></td>
                                </tr>
                                <tr>
                                    <td>WC</td>
                                    <td><%= @vendor.wc %></td>
                                </tr>
                                <tr>
                                    <td>Liability</td>
                                    <td><%= @vendor.liability %></td>
                                </tr>
                                <tr>
                                    <td>Pollution</td>
                                    <td><%= @vendor.pollution %></td>
                                </tr>
                                <tr>
                                    <td>Auto</td>
                                    <td><%= @vendor.auto %></td>
                                </tr>
                                <tr>
                                    <td>W9</td>
                                    <td><%= @vendor.w9 %></td>
                                </tr>
                                <tr>
                                    <td>Indpendent Contractor Agreement</td>
                                    <td><%= @vendor.independent_contractor_agreement %></td>
                                </tr>
                                <tr>
                                    <td>Email</td>
                                    <td><%= @vendor.email %></td>
                                </tr>



                            </tbody>
                        </table>

                    </div>
                </div>
            </div>
            <div class="panel-footer">

                <%= link_to edit_vendor_path(@vendor), class: "btn btn-sm btn-warning", type: "button" do %>
                <i class="glyphicon glyphicon-edit"></i>
                <% end %>

                <%= link_to vendor_uploads_path(@vendor), class: "btn btn-sm btn-warning", type: "button" do %>
                <i class="glyphicon glyphicon-file"></i>
                <% end %>
            </div>
        </div>
    </div>
</div>
