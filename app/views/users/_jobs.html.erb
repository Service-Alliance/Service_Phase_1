<% if jobs.any? %>
  <table class="table table-striped table-condensed table-striped">
    <tr>
      <th>Id</th>
      <th>Status</th>
      <th>Name</th>
      <th>Customer</th>
      <th>Franchise</th>
      <th>Loss Type</th>
      <th>Last Updated</th>
      <th>Coordinator</th>
    </tr>
    <%= render partial: 'job', collection: jobs, as: :job %>
  </table>
<% end %>
