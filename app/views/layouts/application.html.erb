<!DOCTYPE html>
<html>
    <head>
        <title>Service Alliance Inc.</title>
        <%= stylesheet_link_tag    'application', media: 'all' %>
        <%= javascript_include_tag 'application' %>
        <%= csrf_meta_tags %>
    </head>
    <body>
    <% if current_user && (current_user.admin? || current_user.job_coordinator?) %>
        <%= render partial: "layouts/admin_navbar" %>
    <% else %>
        <%= render partial: "layouts/call_rep_navbar" %>
    <% end %>
    <%= yield %>

    </body>
</html>
