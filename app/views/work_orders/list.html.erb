<div class="container-fluid">

  <section id="home">
    <div class="row">

      <div class="col-lg-12">
        <div class="panel panel-default">
          <div class="panel-heading">Work Orders Created</div>
          <div class="panel-body">
            <%= render partial: 'datatables/work_orders', locals: {
              columns: [
                :created_at,
                :updated_at,
                :id,
                :state,
                :job_start,
                :job_id,
                :job_name,
                :typed_by,
                :contact,
                :telephone,
                :job_location,
                :vendor_name,
                :crew_names,
                :referral,
                :required,
                :scope_of_work,
                :estimated_hours,
                :number_of_crew_chiefs,
                :number_of_technicians
              ]}
            %>
        </div>
      </div>
    </div>
  </section>
</div>
