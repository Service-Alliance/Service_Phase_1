<div class="col-xs-12">
    <div class="row content-box">
        <%= form_for [@job, @work_order] do |f| %>
        <% if @work_order.errors.any? %>
        <div id="error_explanation">
            <h2><%= pluralize(@work_order.errors.count, "error") %>
                prohibited this work_order from being saved:</h2>

            <ul>
                <% @work_order.errors.full_messages.each do |message| %>
                <li><%= message %></li>
                <% end %>
            </ul>
        </div>
        <% end %>

        <div class="col-sm-3">

            <div class="form-group">
                <%= f.label :to %><br>
                <%= f.text_field :to, class: "form-control" %>
            </div>
        </div>
        <div class="col-sm-3">
            <div class="form-group">
                <%= f.label :name %><br>
                <%= f.text_field :name, class: "form-control" %>
            </div>
        </div>
        <div class="col-sm-3">
            <div class="form-group">
                <%= f.label :date %><br>
                <%= f.date_select :date %>
            </div>
        </div>
        <div class="col-sm-3">
            <div class="form-group">
                <%= f.label :typed_by %><br>
                <%= f.text_field :typed_by, class: "form-control", value: current_user.full_name %>
            </div>
        </div>
        <div class="col-sm-3">
            <div class="form-group">
                <%= f.label :job_start %><br>
                <%= f.datetime_select :job_start, class: "form-control" %>
            </div>
        </div>
        <div class="col-sm-3">
            <div class="form-group">
                <%= f.label :job_name %><br>
                <%= f.text_field :job_name, class: "form-control" %>
            </div>
        </div>
        <div class="col-sm-3">
            <div class="form-group">
                <%= f.label :job_location %><br>
                <%= f.text_field :job_location, class: "form-control" %>
            </div>
        </div>
        <div class="col-sm-3">
            <div class="form-group">
                <%= f.label :telphone %><br>
                <%= f.text_field :telphone, class: "form-control" %>
            </div>
        </div>
        <div class="col-sm-3">
            <div class="form-group">
                <%= f.label :contact %><br>
                <%= f.text_field :contact, class: "form-control" %>
            </div>
        </div>
        <div class="col-sm-3">
            <div class="form-group">
                <%= f.label :insurance %><br>
                <%= f.text_field :insurance, class: "form-control" %>
            </div>
        </div>
        <div class="col-sm-3">
            <div class="form-group">
                <%= f.label :claim_number %><br>
                <%= f.text_field :claim_number, class: "form-control" %>
            </div>
        </div>
        <div class="col-sm-3">
            <div class="form-group">
                <%= f.label :crew %><br>
                <%= f.text_field :crew, class: "form-control" %>
            </div>
        </div>
        <div class="col-sm-3">
            <div class="form-group">
                <%= f.label :approx_time_on_loss %><br>
                <%= f.time_select :approx_time_on_loss, class: "form-control" %>
            </div>
        </div>
        <div class="col-sm-3">
            <div class="form-group">
                <%= f.label :required %><br>
                <%= f.text_area :required, class: "form-control" %>
            </div>
        </div>
        <div class="col-sm-3">
            <div class="form-group">
                <%= f.label :referral %><br>
                <%= f.text_field :referral, class: "form-control" %>
            </div>
        </div>
        <div class="col-sm-3">
            <div class="form-group">
                <%= f.label :franchise_location %><br>
                <%= f.text_field :franchise_location, class: "form-control" %>
            </div>
        </div>
        <div class="col-sm-3">
            <div class="form-group">
                <%= f.label :scope_of_work %><br>
                <%= f.text_area :scope_of_work, class: "form-control" %>
            </div>
        </div>
        <div class="col-sm-3">
            <div class="form-group">
                <%= f.label :job_manager_contact_info %><br>
                <%= f.text_field :job_manager_contact_info, class: "form-control" %>
            </div>
        </div>
        <div class="actions">
            <%= f.submit %>
        </div>
        <% end %>
    </div>
</div>
