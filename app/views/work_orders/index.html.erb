<p id="notice"><%= notice %></p>

<h1>Listing Work Orders</h1>

<table>
  <thead>
    <tr>
      <th>Job</th>
      <th>To</th>
      <th>Name</th>
      <th>Date</th>
      <th>Typed by</th>
      <th>Job start</th>
      <th>Job name</th>
      <th>Job location</th>
      <th>Telphone</th>
      <th>Contact</th>
      <th>Insurance</th>
      <th>Claim number</th>
      <th>Crew</th>
      <th>Approx time on loss</th>
      <th>Required</th>
      <th>Referral</th>
      <th>Franchise location</th>
      <th>Scope of work</th>
      <th>Job manager contact info</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @work_orders.each do |work_order| %>
      <tr>
        <td><%= work_order.job_id %></td>
        <td><%= work_order.to %></td>
        <td><%= work_order.name %></td>
        <td><%= work_order.date %></td>
        <td><%= work_order.typed_by %></td>
        <td><%= work_order.job_start %></td>
        <td><%= work_order.job_name %></td>
        <td><%= work_order.job_location %></td>
        <td><%= work_order.telphone %></td>
        <td><%= work_order.contact %></td>
        <td><%= work_order.insurance %></td>
        <td><%= work_order.claim_number %></td>
        <td><%= work_order.crew %></td>
        <td><%= work_order.approx_time_on_loss %></td>
        <td><%= work_order.required %></td>
        <td><%= work_order.referral %></td>
        <td><%= work_order.franchise_location %></td>
        <td><%= work_order.scope_of_work %></td>
        <td><%= work_order.job_manager_contact_info %></td>
        <td><%= link_to 'Show', work_order %></td>
        <td><%= link_to 'Edit', edit_work_order_path(work_order) %></td>
        <td><%= link_to 'Destroy', work_order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Work order', new_job_work_order_path(@job) %>
